SleepEntry		PROC		EXPORT
				move.l	d0,-(sp)				; sleep selector
				move.l	a0,-(sp)				; parameter block pointer 
				jsr		Sleep_Task				; call sleep task 
				move.l	(sp)+,a0
				move.l	(sp)+,d0
				clr.l	d0
				rts								;

